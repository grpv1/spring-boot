<settings xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0                       http://maven.apache.org/xsd/settings-1.0.0.xsd">
<pluginGroups>
  </pluginGroups>
          
  <proxies>
  </proxies>
          
  <servers>
    <server>
      <id>gitlab-maven</id>
      <configuration>
        <httpHeaders>
          <property>
            <name>Job-Token</name>
            <value>${CI_JOB_TOKEN}</value>
            <!--name>Private-Token</name>
            <value>${env.GITLAB_PRIVATE_TOKEN}</value-->
          </property>
        </httpHeaders>
      </configuration>
    </server>
    <server>
      <id>gitlab</id>  
      <username>${GITLAB_USER_LOGIN}</username>  
      <password>${GITLAB_PRIVATE_TOKEN}</password>  
    </server>
  </servers>

  <mirrors>
    <mirror>
      <id>dws-all-repos-mirror</id>
      <name>DWS Gitlab Maven</name>
      <url>https://gitlab.com/api/v4/groups/natwestgroup/-/packages/maven</url>
      <mirrorOf>gitlab-maven</mirrorOf>
    </mirror>
  </mirrors>
 
  <profiles>
    <profile>
      <id>artifactory</id>
        <repositories>
            <repository>
             <id>gitlab-maven</id>
             <!--url>${gitlab.instance.mvn.endpoint}</url-->
             <url>https://gitlab.com/api/v4/groups/natwestgroup/-/packages/maven</url>
           </repository>
        </repositories>
    </profile>
  </profiles>

    <!--activeProfiles>
        <activeProfile>artifactory</activeProfile>
    </activeProfiles-->

</settings>
